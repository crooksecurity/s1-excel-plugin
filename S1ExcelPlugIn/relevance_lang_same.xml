<?xml version="1.0" encoding="utf-8"?>
<LexScheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Author>Lee Wei</Author>
  <Name>BigFix Relevance Language</Name>
  <Version>1.5</Version>
  <Styles>
    <Style>
      <Name>Block</Name>
      <ForeColor>DarkGray</ForeColor>
      <FontStyle>Bold</FontStyle>
    </Style>
    <Style>
      <Name>Comment</Name>
      <ForeColor>Green</ForeColor>
      <FontStyle>Italic</FontStyle>
    </Style>
    <Style>
      <Name>Constant</Name>
      <Desc>Such as numbers, true and false</Desc>
      <ForeColor>DarkOrchid</ForeColor>
    </Style>
    <Style>
      <Name>Error</Name>
      <ForeColor>Crimson</ForeColor>
    </Style>
    <Style>
      <Name>Inspector</Name>
      <ForeColor>DarkBlue</ForeColor>
      <FontStyle>Underline</FontStyle>
    </Style>
    <Style>
      <Name>Punctuation</Name>
      <ForeColor>Blue</ForeColor>
      <FontStyle>Bold</FontStyle>
    </Style>
    <Style>
      <Name>Quoted_String</Name>
      <ForeColor>Crimson</ForeColor>
    </Style>
    <Style>
      <Name>ResWord</Name>
      <ForeColor>Blue</ForeColor>
    </Style>
    <Style>
      <Name>Whitespace</Name>
    </Style>
    <Style>
      <Name>Word</Name>
      <ForeColor>Black</ForeColor>
    </Style>
  </Styles>
  <States>
    <State>
      <Name>Default</Name>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>whitespace</Name>
          <LexStyle>8</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\s+</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>block</Name>
          <LexStyle>0</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\(</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>end</Name>
          <LexStyle>0</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\)</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>quote</Name>
          <LexStyle>6</LexStyle>
          <LeaveState>1</LeaveState>
          <Expressions>
            <string>\"</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>number_true_false</Name>
          <LexStyle>2</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\d+|true|false</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment_single</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>2</LeaveState>
          <Expressions>
            <string>//</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment_block</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>3</LeaveState>
          <Expressions>
            <string>/\*</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>nonword</Name>
          <LexStyle>5</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\W</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>word</Name>
          <LexStyle>9</LexStyle>
          <LeaveState>0</LeaveState>
          <ResWordSets>
            <ResWordSet>
              <Name>Resword Set0</Name>
              <ResWordStyle>7</ResWordStyle>
              <ResWords>
                <word>and</word>
                <word>as</word>
                <word>by</word>
                <word>contain</word>
                <word>contained</word>
                <word>contains</word>
                <word>does</word>
                <word>else</word>
                <word>end</word>
                <word>ends</word>
                <word>equal</word>
                <word>equals</word>
                <word>exist</word>
                <word>exists</word>
                <word>greater</word>
                <word>if</word>
                <word>is</word>
                <word>it</word>
                <word>item</word>
                <word>items</word>
                <word>less</word>
                <word>mod</word>
                <word>no</word>
                <word>not</word>
                <word>number</word>
                <word>of</word>
                <word>or</word>
                <word>start</word>
                <word>starts</word>
                <word>than</word>
                <word>the</word>
                <word>then</word>
                <word>there</word>
                <word>to</word>
                <word>whose</word>
                <word>with</word>
              </ResWords>
            </ResWordSet>
          </ResWordSets>
          <Expressions>
            <string>\w+</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>error</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>.</string>
          </Expressions>
        </SyntaxBlock>
      </SyntaxBlocks>
    </State>
    <State>
      <Name>String</Name>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>alphanumeric</Name>
          <LexStyle>6</LexStyle>
          <LeaveState>1</LeaveState>
          <Expressions>
            <string>\w+</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>quote</Name>
          <LexStyle>6</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\"</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>nonword</Name>
          <LexStyle>6</LexStyle>
          <LeaveState>1</LeaveState>
          <Expressions>
            <string>\W</string>
          </Expressions>
        </SyntaxBlock>
      </SyntaxBlocks>
    </State>
    <State>
      <Name>Comment_Single</Name>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>end</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>$</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>everything</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>2</LeaveState>
          <Expressions>
            <string>.*</string>
          </Expressions>
        </SyntaxBlock>
      </SyntaxBlocks>
    </State>
    <State>
      <Name>Comment_Block</Name>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>end</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\*/</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>everything</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>3</LeaveState>
          <Expressions>
            <string>.</string>
          </Expressions>
        </SyntaxBlock>
      </SyntaxBlocks>
    </State>
  </States>
</LexScheme>